<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>

	<package name="trainResult" extends="json-default">
		<action name="updateResultRatio" class="com.thinkPro.train.action.result.ResultRatioAction"
			method="updateResultRatio">
			<result type="json">
				<param name="root">trainResultRatio</param>
			</result>
		</action>
		
		<action name="searchResultRatio" class="com.thinkPro.train.action.result.ResultRatioAction"
			method="searchResultRatio">
			<result type="json">
				<param name="root">trainResultRatio</param>
			</result>
		</action>
	</package>
</struts>